                successful_applications += 1
            elif 'consciousness_enhancements' in domain_results and domain_results['consciousness_enhancements'] > 0:
                successful_applications += 1
            
            # Count consciousness optimizations/enhancements
            if 'consciousness_applicable' in domain_results and domain_results['consciousness_applicable']:
                consciousness_optimizations += 1
            elif 'consciousness_optimizable' in domain_results and domain_results['consciousness_optimizable']:
                consciousness_optimizations += 1
            elif 'consciousness_enhancements' in domain_results:
                consciousness_optimizations += domain_results['consciousness_enhancements']
    
    universal_applicability = successful_applications / domains_tested * 100 if domains_tested > 0 else 0
    
    return {
