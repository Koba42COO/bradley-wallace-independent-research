def wallace_transform(x, alpha=PHI, beta=1.0, epsilon=1e-15):
    """Wallace Transform: W_φ(x) = α log^φ(x + ε) + β"""
    return alpha * np.log(x + epsilon)**PHI + beta

def is_prime(n):
    """Check if number is prime"""
    if n < 2:
        return False
    if n == 2:
        return True
    if n % 2 == 0:
        return False
    for i in range(3, int(n**0.5) + 1, 2):
        if n % i == 0:
            return False
    return True

